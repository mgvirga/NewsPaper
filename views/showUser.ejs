<html class="cool">
    <head>
        <h1>Please enter the new user</h1>
        <style>
            .cool{ background-image: linear-gradient(white, blue) !important;}
            table 
            {
                font-family: arial, sans-serif;
                border-collapse: collapse;
                width: 100%;
            }
            td, th 
            {
                border: 5px solid black;
                text-align: left;
                padding: 20px;
            }
        </style>
    </head>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Username</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        <% posts.forEach(function(post){ %>
            <tr>
                <th><%=post.title%></th>
                <th><%=post.username%></th>
                <th><%=post.password%></th>
            </tr>
        <%});%>
    </tbody>
</table>
<script>
    $(document).ready(function () {
        $('#display').DataTable();
    });
</script>
<br><br><br>
 <ui>
       <li><a href="/showItem">Show Item</a></li>
       <li><a href="/addItem">Add Item</a></li>
       <li><a href="/weather">Weather</a></li>
       <% if(test){ %>

        <li><a href="/showUser">Show User</a></li>
        <li><a href="/addUser">Add User</a></li>
        <% } %>
   </ui>

</html>